{
  "$schema": "https://raw.githubusercontent.com/dotnet/dev-proxy/main/schemas/v2.0.0/mockresponseplugin.mocksfile.schema.json",
  "mocks": [
    {
      "request": {
        "url": "https://jsonplaceholder.typicode.com/posts/1",
        "method": "GET"
      },
      "response": {
        "statusCode": 200,
        "headers": [
          {
            "name": "content-type",
            "value": "application/json; charset=utf-8"
          },
          {
            "name": "x-powered-by",
            "value": "Dev Proxy"
          },
          {
            "name": "x-mocked-by",
            "value": "Dev Proxy MockResponsePlugin"
          }
        ],
        "body": {
          "userId": 1,
          "id": 1,
          "title": "Hello from Dev Proxy!",
          "body": "This response was mocked by Dev Proxy MockResponsePlugin. The original API was not called."
        }
      }
    }
  ]
}
