{
  "$schema": "https://raw.githubusercontent.com/dotnet/dev-proxy/main/schemas/v0.29.2/crudapiplugin.schema.json",
  "baseUrl": "https://api.northwind.com/suppliers",
  "dataFile": "suppliers-data.json",
  "actions": [
    {
      "action": "getAll"
    },
    {
      "action": "getOne",
      "url": "/{supplier-id}",
      "query": "$.[?(@.SupplierID == {supplier-id})]"
    },
    {
      "action": "create"
    },
    {
      "action": "merge",
      "url": "/{supplier-id}",
      "query": "$.[?(@.SupplierID == {supplier-id})]"
    },
    {
      "action": "delete",
      "url": "/{supplier-id}",
      "query": "$.[?(@.SupplierID == {supplier-id})]"
    }
  ]
}